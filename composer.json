{
    "name": "mam/ali-task",
    "description": "MAM Sales Board",
    "minimum-stability": "stable",
    "license": "GPL-2.0-or-later",
    "keywords": [
        "wordpress",
        "plugin"
    ],
    "type": "wordpress-plugin",
    "homepage": "https://github.com/Watchout1992/mam-sales-board/",
    "authors": [
        {
            "name": "Ali",
            "email": "ali.alsalti1992@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Watchout1992/mam-sales-board/issues"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "brain/monkey": "^2",
        "wp-coding-standards/wpcs": "^2",
        "phpcompatibility/phpcompatibility-wp": "^2",
        "dg/composer-cleaner": "^2",
        "phpcompatibility/php-compatibility": "^9.1"
    },
    "scripts": {
        "post-install-cmd": [
            "@phpcs"
        ],
        "post-update-cmd": [
            "@phpcs"
        ],
        "phpcs": "./vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs,vendor/phpcompatibility/php-compatibility/PHPCompatibility,vendor/phpcompatibility/phpcompatibility-wp/PHPCompatibilityWP,vendor/phpcompatibility/phpcompatibility-paragonie/PHPCompatibilityParagonieRandomCompat,vendor/phpcompatibility/phpcompatibility-paragonie/PHPCompatibilityParagonieSodiumCompat"
    },
    "autoload": {
        "psr-4": {
            "Mam\\AliTask\\": "inc/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mam\\AliTask\\Tests\\": "tests/UnitTests/",
            "Mam\\AliTask\\": "inc/"
        }
    }
}